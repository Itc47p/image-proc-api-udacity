{
    "name": "harrington-image-processor",
    "version": "1.0.0",
    "description": "A Node.js package for image processing using the Harrington algorithm.",
    "author": "Itc47p",
    "type": "module",
    "main": "index.ts",
    "scripts": {
        "build": "npx tsc",
        "start": "nodemon --exec ts-node build/src/index.js",
        "build:start": "npx tsc && nodemon --exec ts-node build/src/index.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Itc47p/image-proc-api-udacity.git"
    },
    "keywords": [],
    "devDependencies": {
        "@types/eslint": "^9.6.1",
        "@types/express": "^5.0.3",
        "@types/jasmine": "^5.1.9",
        "@types/node": "^24.3.1",
        "@types/prettier": "^2.7.3",
        "jasmine": "^5.12.0",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.2"
    },
    "watch": {
        "build-watch": {
            "patterns": [
                "src/**/*.ts"
            ],
            "extensions": "ts",
            "quiet": true,
            "runOnChangeOnly": true
        }
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Itc47p/image-proc-api-udacity/issues"
    },
    "homepage": "https://github.com/Itc47p/image-proc-api-udacity#readme",
    "dependencies": {
        "eslint": "^9.36.0",
        "express": "^5.1.0",
        "prettier": "^3.6.2",
        "sharp": "^0.34.4",
        "typescript": "^5.9.2"
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=6.0.0"
    }
}
